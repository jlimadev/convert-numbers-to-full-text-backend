{
  "name": "spell-out-numbers-ts",
  "author": "Jonathan Lima",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "build:watch": "yarn build --watch",
    "start": "yarn build && node dist/main/server.js",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "debug:watch": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "start:dev": "sucrase-node src/main/server.ts",
    "test": "jest --runInBand",
    "test:cov": "yarn test --coverage",
    "lint": "eslint --config=.eslintrc.json src/**/*.ts",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "lite-server": "^2.6.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "typescript": "^4.5.2"
  }
}
